# Copy this file to .env and fill in your actual API keys and configuration

# Beyond Presence API Configuration
BEYOND_PRESENCE_API_KEY=your-beyond-presence-api-key-here

# LiveKit Configuration (optional - for real-time video sessions)
LIVEKIT_URL=wss://your-domain.livekit.cloud
LIVEKIT_TOKEN=your-livekit-token-here

# Mistral AI Configuration
MISTRAL_API_KEY=your-mistral-api-key-here

# Other AI Service Keys (add as needed)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Application Configuration
DEFAULT_AVATAR_ID=b9be11b8-89fb-4227-8f86-4a881393cbdb
DEFAULT_SESSION_LENGTH_MINUTES=5
DEFAULT_LANGUAGE=en

# Environment
ENVIRONMENT=development 

# Weaviate Cloud Configuration
WEAVIATE_URL=https://your-cluster-name.weaviate.network
WEAVIATE_API_KEY=your-weaviate-api-key-here
WEAVIATE_GRPC_URL=grpc-your-cluster-name.weaviate.network:443

# AI Assistant Configuration
WEAVIATE_COLLECTION_NAME=Candidates
WEAVIATE_AUTO_SYNC=true
WEAVIATE_EMBEDDING_MODEL=text-embedding-ada-002
AI_ASSISTANT_MODEL=mistral-small-latest
AI_ASSISTANT_MAX_RESULTS=5 